@#include"header.h"
#include"prototype.h"
int j=200;
int insertbeg(struct node *strt)
{
	struct node *temp;
	temp = creatnode();
	temp->Next = strt->Next;
	strt->Next = temp;
	temp->info = j;
	j++;
	printf("info of the new node %d \n",j);
	printf("NODE IS INSERTED AT BEGINNING\n");
	return j;
}
